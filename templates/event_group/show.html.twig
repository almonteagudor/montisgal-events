{% extends 'base.html.twig' %}

{% block title %}EventGroup{% endblock %}

{% block body %}
    <h1>EventGroup</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ event_group.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ event_group.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ event_group.description }}</td>
            </tr>
            <tr>
                <th>Public</th>
                <td>{{ event_group.public ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_event_group_index') }}">back to list</a>

    <a href="{{ path('app_event_group_edit', {'id': event_group.id}) }}">edit</a>

    {{ include('event_group/_delete_form.html.twig') }}
{% endblock %}
